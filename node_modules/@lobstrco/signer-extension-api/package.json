{
    "name": "@lobstrco/signer-extension-api",
    "version": "2.0.0",
    "license": "GPL-3.0",
    "author": "Ultra Stellar, LLC.",
    "description": "Utility functions to interact with LOBSTR extension",
    "repository": {
        "url": "git+ssh://git@github.com/Lobstrco/lobstr-browser-extension.git",
        "type": "git"
    },
    "main": "build/index.min.js",
    "files": [
        "build/index.min.js",
        "build/@lobstrco/signer-extension-api/src/*.d.ts",
        "README.md",
        "CHANGELOG.MD"
    ],
    "scripts": {
        "prepare": "yarn build",
        "build": "rm -rf build && webpack --config webpack.production.js --mode production && tsc --emitDeclarationOnly --declaration --outDir build",
        "start": "webpack --config webpack.dev.js --watch --mode development"
    },
    "types": "build/@lobstrco/signer-extension-api/src/index.d.ts",
    "bugs": {
        "url": "https://github.com/Lobstrco/lobstr-browser-extension/issues"
    },
    "homepage": "https://github.com/Lobstrco/lobstr-browser-extension/blob/main/%40lobstrco/signer-extension-api#readme"
}
